.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to t("web-app-theme.list", :default => "List"), admin_competitions_path
      %li.active= link_to t("web-app-theme.new", :default => "New"), new_admin_competition_path
  .content
    %h2.title #{ t("web-app-theme.new", :default => "New")} Competition
    .inner
      - form_for @competition, :url => admin_competitions_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}

- content_for :sidebar, render(:partial => 'sidebar')